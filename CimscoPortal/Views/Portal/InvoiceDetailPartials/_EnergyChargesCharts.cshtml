<div class="databox databox-xlg radius-bordered databox-shadowed databox-graded databox-vertical" style="height:230px;">
    <h6 class="row-title before-blue">{{nrgData.headerData.header}}</h6>
    <span class="pull-right databox-number lightcarbon">Total: ${{nrgData.totalLoss + nrgData.totalCost | number:2}}&nbsp;</span>
    <div class="databox-row bg-white">
        <div style="margin: 0 0 4px;max-height: 1px;min-height: 1px;overflow: hidden;"></div>
        <div id="{{$index + 1}}SlineComposite" class="databox-sparkline no-margin cp-graph-element">
            <span data-sparkline="compositebar" data-height="100px" data-width="100%"
                  data-barcolor="#b0dc81"
                  data-barwidth="15px" data-barspacing="30px"
                  data-fillcolor="false" data-linecolor="#fff" data-spotradius="3" data-linewidth="2"
                  data-spotcolor="#fafafa" data-minspotcolor="#fafafa" data-maxspotcolor="#fff"
                  data-highlightspotcolor="#fff" data-highlightlinecolor="#fff"
                  data-maxvalueforbar="{{slModel.maxYbar}}"
                  data-minvalueforbar="{{slModel.minYbar}}"
                  data-maxseriesb="{{slModel.maxYgraph}}"
                  data-minseriesb="{{slModel.minYgraph}}"
                  data-seriesb="{{nrgData.energyRateByBracket}}"
                  data-seriesa="{{nrgData.energyChargeByBracket}}"
                  class="SlineComposite">
            </span>
        </div>
    </div>
    <div class="databox-bottom no-padding bordered-thick bordered-darkblue">
        <div class="databox-row">
            <div class="databox-cell cell-4 no-padding text-align-center bordered-right bordered-platinum">
                <span class="databox-number lightcarbon no-margin">${{nrgData.totalCost | number:2}}</span>
                <span class="databox-text sonic-silver  no-margin">Total energy charge</span>
            </div>
            <div class="databox-cell cell-4 no-padding text-align-center bordered-right bordered-platinum">
                <span class="databox-number lightcarbon no-margin">{{nrgData.maxRate | number:2}}c</span>
                <span class="databox-text sonic-silver no-margin">Max cents per KWh</span>
            </div>
            <div class="databox-cell cell-4 no-padding text-align-center">
                <span class="databox-number lightcarbon no-margin">${{nrgData.totalLoss | number:2}} ({{nrgData.lossRate * 100 | number:2}}%)</span>
                <span class="databox-text sonic-silver no-margin">Losses</span>
            </div>
        </div>
    </div>
</div>
